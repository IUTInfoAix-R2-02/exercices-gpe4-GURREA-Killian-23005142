<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane alignment="CENTER" hgap="100" styleClass="root" stylesheets="@Login.css" vgap="20" fx:controller="fr.amu.iut.exercice8.LoginControl"
          xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <Text id="welcome-text" text="Welcome"/>
   <Label text="Password:" GridPane.rowIndex="2"/>

   <Label text="User Id:" GridPane.rowIndex="1"/>
   <TextField fx:id="userId" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
   <PasswordField fx:id="pwd" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
   <HBox alignment="CENTER" prefHeight="0.0" prefWidth="306.0" spacing="20.0" GridPane.columnSpan="2"
         GridPane.rowIndex="3">
      <Button fx:id="okBtn" onAction="#okClicked" mnemonicParsing="false" text="Ok"/>
      <Button fx:id="cancelBtn" onAction="#cancelClicked" mnemonicParsing="false" text="Cancel"/>
   </HBox>

   <!--  à compléter pour les boutons -->

   <columnConstraints>
      <ColumnConstraints maxWidth="103.0" minWidth="63.0" prefWidth="83.0"/>
      <ColumnConstraints maxWidth="143.0" minWidth="103.0" prefWidth="123.0"/>
   </columnConstraints>
   <opaqueInsets>
      <Insets/>
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0"/>
   </padding>
   <rowConstraints>
      <RowConstraints/>
      <RowConstraints/>
      <RowConstraints/>
      <RowConstraints/>
   </rowConstraints>
</GridPane>
